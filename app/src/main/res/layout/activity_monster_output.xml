<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MonsterOutput"
    android:background="@drawable/backgroundone">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="World of FrostBite"
        android:textSize="45dp"
        android:fontFamily="@font/thewildbreath"
        android:textColor="#F2F0F0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <ImageView
        android:id="@+id/charImage4"
        android:layout_width="281dp"
        android:layout_height="253dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/wyvern"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/classout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/thewildbreath"
        android:text="job here"
        android:textColor="#F2F0F0"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="@+id/nameout2"
        app:layout_constraintStart_toStartOf="@+id/nameout2"
        app:layout_constraintTop_toBottomOf="@+id/nameout2" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:background="@null"
        android:orientation="horizontal"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView
            android:id="@+id/matkout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="matk"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/pdefout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="pdef"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/mdefout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="mdef"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:layout_marginTop="28dp"
        android:background="@null"
        android:orientation="horizontal"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/classout2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/classout2"
        app:layout_constraintTop_toBottomOf="@+id/classout2">

        <TextView
            android:id="@+id/hpout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="hpHere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/mpout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="mphere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/patkout2"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="patk"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/nameout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/thewildbreath"
        android:text="name here"
        android:textColor="#F2F0F0"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="@+id/charImage4"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/charImage4"
        app:layout_constraintTop_toBottomOf="@+id/charImage4" />

    <TextView
        android:id="@+id/evaout2"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/thewildbreath"
        android:text="eva"
        android:textColor="#F2F0F0"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/linearLayout2"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

</androidx.constraintlayout.widget.ConstraintLayout>