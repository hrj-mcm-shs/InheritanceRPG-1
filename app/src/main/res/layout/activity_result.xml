<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Result"
    android:background="@drawable/backgroundone">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="World of FrostBite"
        android:textSize="40dp"
        android:fontFamily="@font/thewildbreath"
        android:textColor="#F2F0F0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.042" />

    <ImageView
        android:id="@+id/charImage2"
        android:layout_width="281dp"
        android:layout_height="253dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/warrior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <LinearLayout
        android:layout_width="253dp"
        android:layout_height="52dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout3"
        app:layout_constraintStart_toStartOf="@+id/linearLayout3"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <TextView
            android:id="@+id/matkout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="matk"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/mdefout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="mdef"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/classout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/thewildbreath"
        android:text="job here"
        android:textColor="#F2F0F0"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="@+id/nameout"
        app:layout_constraintStart_toStartOf="@+id/nameout"
        app:layout_constraintTop_toBottomOf="@+id/nameout" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:background="@null"
        android:orientation="horizontal"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView
            android:id="@+id/strout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="strhere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/agiout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="agihere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/intout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="inthere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:background="@null"

        android:orientation="horizontal"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/linearLayout2"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/patkout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="patk"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/pdefout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="pdef"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/evaout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="eva"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="345dp"
        android:layout_height="52dp"
        android:layout_marginTop="28dp"
        android:background="@null"
        android:orientation="horizontal"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="@+id/classout"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/classout"
        app:layout_constraintTop_toBottomOf="@+id/classout">

        <TextView
            android:id="@+id/hpout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="hpHere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/mpout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="mphere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/levelout"
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/thewildbreath"
            android:text="lvlhere"
            android:textColor="#F2F0F0"
            android:textSize="20dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/nameout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/thewildbreath"
        android:text="name here"
        android:textColor="#F2F0F0"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="@+id/charImage2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/charImage2"
        app:layout_constraintTop_toBottomOf="@+id/charImage2" />


</androidx.constraintlayout.widget.ConstraintLayout>